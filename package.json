{
  "name": "grade-curricular",
  "version": "1.0.0",
  "description": "http://grade-curricular.herokuapp.com/",
  "scripts": {
    "start": "npm run install-all & npm run start-client & npm run start-server &",
    "start-dev": "npm run install-all & npm run start-client-dev & npm run start-server-dev &",
    "install-all": "npm run install-server & npm run install-client &",
    "install-server": "cd ./server && npm install",
    "install-client": "cd ./client && npm install",
    "start-server": "cd ./server && node server.js",
    "start-client": "cd ./client && ng build",
    "start-server-dev": "cd ./server && nodemon server.js",
    "start-client-dev": "cd ./client && ng build --watch"
  },
  "dependencies": {
    "standard": "^14.3.1"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelndm/grade-curricular.git"
  },
  "license": "ISC",
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  }
}